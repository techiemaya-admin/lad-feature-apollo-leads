{
  "feature": {
    "key": "apollo-leads",
    "name": "Apollo Leads",
    "version": "1.0.0",
    "description": "Apollo.io integration for B2B lead generation and company data enrichment"
  },
  "frontend": {
    "components": [
      "ApolloLeadsSearch.tsx",
      "ApolloCompanyCard.tsx",
      "ApolloEmployeeList.tsx"
    ],
    "hooks": [
      "useApolloLeads.ts",
      "useApolloSearch.ts",
      "useApolloCredits.ts"
    ],
    "services": [
      "apolloLeadsService.ts",
      "apolloPhoneService.ts"
    ],
    "types": [
      "apollo.types.ts"
    ]
  },
  "backend": {
    "endpoints": [
      "POST /api/apollo-leads/search",
      "GET /api/apollo-leads/leads/:id/email",
      "GET /api/apollo-leads/leads/:id/phone",
      "GET /api/apollo-leads/health"
    ]
  },
  "credits": {
    "search": 1,
    "email_reveal": 1,
    "phone_reveal": 8
  },
  "tiers": {
    "free": {
      "enabled": false,
      "message": "Apollo Leads is available on Premium and Enterprise plans"
    },
    "basic": {
      "enabled": false,
      "message": "Apollo Leads is available on Premium and Enterprise plans"
    },
    "premium": {
      "enabled": true,
      "credits": 1000
    },
    "enterprise": {
      "enabled": true,
      "credits": 10000
    }
  }
}
